<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <title>{{ title | d('Lab Page')}}</title>
    {% block stylesheets %}
    <link rel="stylesheet" href="/global.css">
    {% endblock %}
    {% include '_favicon.njk' %}
    <script nomodule>/*@cc_on document.write('\x3Cscript id="_iealwn_js" src="vendor/old-ie-warn.js">\x3C/script>'); @*/</script>
  </head>
  <body>
    <main id="app"></main>
    <script nomodule crossorigin="anonymous" src="https://polyfill.io/v3/polyfill.min.js?flags=gated&features=default%2Cfetch"></script>
    <script>
      try {
        new Function('import("./esm/{{ entry }}.js")')()
      } catch (e) {
        var s = document.createElement('script')
        s.src = 'vendor/shimport.js'
        s.dataset.main = "./es5/{{ entry }}.js"
        document.head.appendChild(s)
      }
    </script>
  </body>
</html>
